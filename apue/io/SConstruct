env = Environment()
env.Append(CPPFLAGS = ['-Wall', '-g', '--std=c++0x'])
env.Append(CPPPATH = ['/usr/local/include'])
env.Program('stdio_demo', 'stdiodemo.cpp')
env.Program('fcntl_demo', 'fcntldemo.cpp')
env.Program('fstat_demo', 'fstatdemo.cpp')
env.Program('readdir_demo', 'readdirdemo.cpp')
env.Program('fileio_demo', 'fileiodemo.cpp')
